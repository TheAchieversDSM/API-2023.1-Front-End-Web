    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/js-cookie": "^3.0.3",
        "@types/moment-range": "^4.0.0",
        "@types/node": "^16.18.14",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "axios": "^1.3.4",
        "bootstrap": "^5.2.3",
        "buffer": "^6.0.3",
        "cypress": "^12.12.0",
        "date-fns": "^2.29.3",
        "highcharts": "^10.3.3",
        "highcharts-react-official": "^3.2.0",
        "js-cookie": "^3.0.1",
        "moment": "^2.29.4",
        "moment-range": "^4.0.2",
        "nookies": "^2.5.2",
        "randomcolor": "^0.6.2",
        "react": "^18.2.0",
        "react-bootstrap": "^2.7.2",
        "react-dom": "^18.2.0",
        "react-icons": "^4.8.0",
        "react-router-dom": "^6.9.0",
        "react-scripts": "5.0.1",
        "react-select": "^5.7.0",
        "react-toastify": "^9.1.2",
        "sweetalert2": "^11.7.3",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "bash build-script.sh && react-scripts -r @cypress/instrument-cra start",
        "sonar:build": "node sonar-project.js",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "nyc:coverage-report": "nyc report --reporter=lcov"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.10.6",
        "@cypress/instrument-cra": "^1.4.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/jest": "^29.5.1",
        "@types/jsonwebtoken": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "babel-plugin-istanbul": "^6.1.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "eslint": "^8.37.0",
        "eslint-plugin-react": "^7.32.2",
        "jest": "^29.5.0",
        "nyc": "^15.1.0",
        "sonarqube-scanner": "^3.0.1",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1"
    },
    "jest": {
        "collectCoverage": true,
        "coverageReporters": [
            "lcov"
        ],
        "coverageDirectory": "coverage",
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ]
    },
    "nyc": {
        "all": true,
        "extends": "@istanbuljs/nyc-config-typescript",
        "report-dir": "coverage",
        "include": [
            "src/**/*.ts",
            "src/**/*.tsx"
        ],
        "exclude": [
            "cypress/",
            "assets/",
            "test/"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "reporter": [
            "text",
            "lcov",
            "html"
        ]
    }
}
